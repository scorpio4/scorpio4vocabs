@prefix rdf:		<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:		<http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:		<http://www.w3.org/2002/07/owl#> .
@prefix dc:			<http://purl.org/dc/elements/1.1/> .
@prefix eg:			<http://www.example.org/> .
@prefix xsd: 		<http://www.w3.org/2001/XMLSchema#>.

@prefix vega: 		<http://scorpio4.com/v1/vocab/vega/>.
@prefix : 		    <http://scorpio4.com/v1/vocab/vega/>.

:Transform
	a					rdfs:Class;
	rdfs:label			"Data Transform";
	rdfs:comment 		"operations on a data set prior to visualization";
	rdfs:isDefinedBy	:;
.


:keys
	rdfs:label		"key";
	rdfs:comment	"key of the transform"
	rdfs:range		xsd:anyURI;
	rdfs:domain		:Transform;
.

:type
	rdfs:label		"type";
	rdfs:comment	"type of the transform"
	rdfs:range		xsd:anyURI;
	rdfs:domain		:Transform;
.

:value
	rdfs:label		"value";
	rdfs:comment	"value of the transform"
	rdfs:range		xsd:anyURI;
	rdfs:domain		:Transform;
.

